    "nuclear-reset": "node scripts/nuclear-template-reset.js",
    "seed-global-ai-brain": "node scripts/seed-global-ai-brain.js",
    "check:routes": "node scripts/check-routes.js",
    "smoke-test": "node scripts/smoke-test.js",
    "validate": "npm run check:routes && npm run smoke-test",
    "bundle:snapshot": "node scripts/bundle-snapshot.js",
    "bundle:check": "node scripts/bundle-check.js",
    "dead": "echo 'dead-code scan placeholder (knip/ts-prune not installed)' && exit 0",
    "guard:single-adapter": "scripts/guard-single-adapter.sh",
    "guard:registry-coverage": "node scripts/guard-registry-coverage.js",
    "guard:tenant-context": "node scripts/guard-tenant-context.js",
    "guards": "scripts/run-guards.sh"
  },
