    "mustache": "^4.2.0",
    "node-cron": "^3.0.3",
    "node-fetch": "^2.7.0",
    "nodemailer": "^6.10.1",
    "openai": "^4.104.0",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "redis": "^5.5.6",
    "stopwords": "^0.0.9",
    "twilio": "^5.8.0",
    "ulid": "^3.0.1",
    "uuid": "^10.0.0",
