    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^7.2.1",
    "eslint-plugin-security": "^3.0.1",
    "jest": "^29.7.0",
    "jsdom": "^26.1.0",
    "nodemon": "^3.0.1",
    "supertest": "^6.3.3",
    "tailwindcss": "^3.4.0"
  },
  "jest": {
    "testMatch": [
      "**/tests/**/*.[jt]s?(x)"
    ]
  }
